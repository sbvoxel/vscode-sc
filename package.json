{
    "name": "vscode-sc",
    "displayName": "Sc Language",
    "description": "Language support for the Sc programming language",
    "version": "0.1.1",
    "engines": {
        "vscode": "^1.84.2"
    },
    "categories": ["Programming Languages"],
    "contributes": {
    "languages": [
        {
            "id": "sc",
            "extensions": [".sc"],
            "configuration": "./language-configuration.json"
        }
    ],
    "grammars": [
        {
            "language": "sc",
            "scopeName": "source.sc",
            "path": "./sc.tmGrammar.json"
        }
    ]
    }
}
